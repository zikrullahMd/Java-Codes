import java.util.*;
import java.math.*;
import java.util.Map.Entry;
public class Misc
{
    public static void main(String[] args)
    {
        Scanner in = new Scanner(System.in);
        long n = in.nextInt();
        int count = 0;
        for(int i =0;i<Math.pow(2, 32);i++) {
        	if(Integer.toBinaryString(i).length() == 4) {
        		count++;
        	}
        }
        System.out.println(count);
    }
}





























