import java.util.*;
import java.math.*;
import java.util.Map.Entry;
class Misc{
	
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        String[] a = new String[n];
        for(int i = 0;i<n;i++) {
        	a[i]  = in.next();
        }
        Stack<Integer> st = new Stack<>();
        int res = 0;
        for(int i = 0;i<n;i++) {
        	String s = a[i];
        	if(s != "+" && s != "-" && s != "/" && s != "*") {
        		st.push(Integer.parseInt(a[i]));
        	}else {
        		int one = st.pop();
        		int two = st.pop();
        		switch(s) {
        		case "+" : res = one + two;
        		break;
        		case "-" : res = one - two;
        		break;
        		case "*" : res = one * two;
        		break;
        		case "/" : res = Math.max(one, two) / Math.min(one, two);
        		break;
        		}
        	}
        }
        System.out.println(res);
    }
}
