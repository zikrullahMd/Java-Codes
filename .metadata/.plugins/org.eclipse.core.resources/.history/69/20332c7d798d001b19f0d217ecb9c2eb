package Codeforces_750;
import java.util.*;
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in  = new Scanner(System.in);
		long t = in.nextLong();
		while(t-->0) {
			long n = in.nextLong();
			long m = in.nextLong();
			long x = in.nextLong();
			long[][] a = new long[(int)n][(int)m];
			int f = 1;
			for(long i = 0;i<n;i++) {
				for(long j = 0;j<m;j++) {
					a[(int)i][(int)j] = f++;
				}
			}
		}
	}

}
