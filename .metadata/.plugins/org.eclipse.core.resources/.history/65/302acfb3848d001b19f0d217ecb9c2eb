package Codeforces_750;
import java.util.*;
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in  = new Scanner(System.in);
		long t = in.nextLong();
		while(t-->0) {
			long n = in.nextLong();
			long m = in.nextLong();
			long x = in.nextLong();
			long row = x % n;
			if(row == 0) {
				row = n;
			}
			long col = x / n;
			System.out.println("col "+col +" "+"row "+row);
			long ans = (col - 1) * m + row;
			System.out.println(ans);
		}
	}

}
