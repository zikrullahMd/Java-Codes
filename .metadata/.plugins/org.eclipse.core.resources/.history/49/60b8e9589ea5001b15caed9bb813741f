import java.io.*;
import java.util.*;

public class Misc {

    public static void main(String[] arga) {
         Scanner in = new Scanner(System.in);
         long t = in.nextLong();
         while(t-->0) {
        	 long n = in.nextLong();
        	 System.out.println((n<<1)+1);
        	 //System.out.println(count);
         }
    }
}