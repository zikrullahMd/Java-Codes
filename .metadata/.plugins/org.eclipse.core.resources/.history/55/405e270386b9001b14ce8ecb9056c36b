import java.util.*;
import java.math.*;
import java.util.Map.Entry;
public class Misc
{
    public static void main(String[] args)
    {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        BigInteger a;
        a = new BigInteger("2");
        BigInteger res = a.pow(n);
        System.out.println(res);
    }
}





























